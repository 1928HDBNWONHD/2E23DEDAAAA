<!DOCTYPE html>
<html>
<head>
    <title>Content Loader</title>
    <script>
        fetch('https://raw.githubusercontent.com/1928HDBNWONHD/2E23DEDAAAA/refs/heads/main/EaglercraftX_1.8_Offline_en_US.html')
            .then(response => response.text())
            .then(html => {
                document.open();
                document.write(html);
                document.close();
            })
            .catch(error => {
                document.body.innerHTML = '<h1>Error loading content</h1><p>' + error + '</p>';
            });
    </script>
</head>
<body>
    <h1>Loading content...</h1>
</body>
</html>
